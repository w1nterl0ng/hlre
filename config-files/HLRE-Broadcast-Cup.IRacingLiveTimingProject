.overlay&gt;.start-lights {
    display: none;
}


/* The following list represents the different components available on your overlay. Adjust the top/bottom/left/right
figures to move the component around the overlay and use the "TranslateZ" value to scale the component up or down!*/

.overlay&gt;.nascar-ticker-2020 {
    grid-template-rows: 0 0 54px 48px 33px;
}

.overlay&gt;.standings { /* Timing Tower - Shows live standings*/
    height: 800px;
    top: 25px;
    transform: perspective(1000px) translateZ(0px);
}

.overlay&gt;.driver-details { /* Driver Details - Shows driver and team name etc*/
    left: 20px;
    bottom: 40px;
    transform: perspective(1000px) translateZ(200px);
}

.overlay&gt;.track-map { /* Track map - Shows car positions on track*/
    right: 10px;
    top: 200px;
    transform: perspective(1000px) translateZ(-150px);
}

.overlay&gt;.twitch-chat { /* Twitch Chat - Shows your Twitch chat on stream */
    position: absolute;
    right: 20px;
    bottom: 200px;
    width: 500px;
    height: 350px;
}

.overlay&gt;.battle { /* Battle Box - Shows other drivers around you and gaps*/
    bottom: 200px;
    right: 20px;
    transform: perspective(1000px) translateZ(0px);

}

.overlay&gt;.dashboard { /* Dashboard - Shows current speed and gear selection*/
    right: 20px;
    bottom: 40px;
    transform: perspective(1000px) translateZ(0px);
}

.overlay&gt;.weather { /* Weather - Shows current weather information*/
    right: 20px;
    bottom: 260px;
    transform: perspective(1000px) translateZ(0px);
}

.overlay&gt;.time-delta-history { /* Time Delta History - Shows gap from previous laps between selected drivers*/
    right: 20px;
    bottom: 200px;
    transform: perspective(1000px) translateZ(0px);
}

.overlay&gt;.qualifying { /* Qualifying Mode - Shows sector and lap time improvements from different drivers*/
    bottom: 40px;
    transform: perspective(1000px) translateZ(0px);
}

.overlay&gt;.interview { /* Interview Label - Additional label above driver details. Label can be edited*/
    left: 20px;
    bottom: 130px;
    transform: perspective(1000px) translateZ(0px);
}

.overlay&gt;.classes { /* Classes - Displays the different classes in session*/
    top: 20px;
    left: 0px;
    right: 0px;
    transform: perspective(1000px) translateZ(0px);
}

.overlay&gt;.lap-time-history { /* Lap Time History - Displays lap time difference from previous laps for selected drivers*/
    bottom: 200px;
    transform: perspective(1000px) translateZ(0px);
}

.overlay&gt;.sectors { /* Sectors - Shows sector time difference between selected drivers*/
    bottom: 200px;
    transform: perspective(1000px) translateZ(0px);
}


.overlay&gt;.race-control { /* Race Control - Displays a message of your choice on screen. Useful for leagues using live stewarding.*/
    bottom: 200px;
    transform: perspective(1000px) translateZ(0px);
}

.overlay&gt;.radio-channel { /* Radio - Displays a graphic when drivers speak in All Chat*/
    top: 550px;
    right: 20px;
    transform: perspective(1000px) translateZ(0px);
}

.overlay&gt;.statistics { /* Statistics - Displays customisable html code, by default a basic table*/
    top: 200px;
    right: 20px;
    width: 700px;
}

.overlay&gt;.pit-stop-history { /* Statistics - Displays customisable html code, by default a basic table*/
    bottom: 200px;

}

